{
	"env": {
		"browser": false,
		"commonjs": false,
		"es2021": true,
		"node": true,
		"es6": true
	},
	"globals": {},
	"extends": ["eslint:recommended"],
	"overrides": [],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"plugins": [],
	"ignorePatterns": ["test.js"],
	"rules": {
		"indent": ["warn", "tab"],
		"quotes": ["warn", "single"],
		"semi": ["warn", "always"],
		"no-unused-labels": "warn"
	}
}
